{
  "interceptor": {
    "shouldBeIgnore": [
      "value",
      "sys",
      "\u00020",
      "windowbtn",
      "00_プロローグ１",
      "menu",
      "WndDisp"
    ],
    "ignoreAsciiOnly": false,
    "maxLength": 1000
  },
  "modifier": {
    "removeAscii": true,
    "deduplicate": true,
    "delineBreak": true
  },
  "merger": {
    "enable": true,
    "timeout": 500
  }
}
